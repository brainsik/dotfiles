[core]
	editor = vim
[push]
	default = current
[pull]
	rebase = true
[alias]
	amend = commit --amend
	br = --no-pager branch
	branch = --no-pager branch
	co = checkout
	dif = diff
	st = status
	stat = status
[difftool]
	prompt = false
[color]
	ui = true
[diff]
	color = true
[color "diff"]
	new = bold blue
	whitespace = normal yellow
[color "status"]
	changed = magenta
	untracked = bold yellow
[transfer]
	fsckObjects = true
[help]
	autocorrect = 20
